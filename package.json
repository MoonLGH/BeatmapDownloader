{
  "name": "beatmapdl",
  "productName": "Beatmap DL",
  "version": "1.0.1",
  "description": "App To Download Beatmaps",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "build": "electron-packager --out dist --overwrite --platform win32 --appname clientlmcenter . --icon=./src/favicon.ico",
    "lint": "echo \"No linting configured\"",
    "build-installer": "electron-builder"
  },
  "build": {
    "appId": "Beatmap DL",
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "src/public/favicon.ico"
    },
    "nsis": {
      "installerIcon": "src/public/favicon.ico",
      "uninstallerIcon": "src/public/favicon.ico",
      "uninstallDisplayName": "Beatmap DL - Uninstaller",
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "keywords": [],
  "author": {
    "name": "MoonLGH",
    "email": "MoonLMC1212@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "Beatmap DL"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "axios": "^0.21.1",
    "cheerio": "^1.0.0-rc.5",
    "electron-squirrel-startup": "^1.0.0",
    "fs": "0.0.1-security",
    "jquery": "^3.5.1",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.53",
    "@electron-forge/maker-deb": "^6.0.0-beta.53",
    "@electron-forge/maker-rpm": "^6.0.0-beta.53",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.53",
    "@electron-forge/maker-zip": "^6.0.0-beta.53",
    "electron": "^10.1.3",
    "electron-builder": "^22.9.1"
  }
}
